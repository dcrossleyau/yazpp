SHELL=/bin/sh
MAKE=make

.PHONY: all doc clean distclean depend

all:
	cd ../src; $(MAKE)

depend:
	cd ../src; $(MAKE) depend
doc:
	cd ../include; doc++ -H -d ../doc *.h

clean:
	cd ../src; $(MAKE) clean

distclean: clean
	rm -f ../src/Makefile Makefile config.*
