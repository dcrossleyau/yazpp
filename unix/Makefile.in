SHELL=/bin/sh
MAKE=make

.PHONY: all doc clean distclean

all:
	cd ../src; $(MAKE)

doc:
	cd ../include; doc++ -H -d ../doc *.h

clean:
	cd ../src; $(MAKE) clean

distclean: clean
	rm -f ../src/Makefile Makefile config.*
